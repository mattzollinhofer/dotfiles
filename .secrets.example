# API Tokens and Sensitive Environment Variables
# Copy this file to ~/.secrets and add your actual values
# chmod 600 ~/.secrets

# Shortcut API token
# Get from: https://app.shortcut.com/settings/account/api-tokens
export SHORTCUT_API_TOKEN=your_token_here

# Figma access token
# Get from: https://www.figma.com/developers/api#access-tokens
export FIGMA_ACCESS_TOKEN=your_token_here

# Sentry auth token
# Get from: https://sentry.io/settings/account/api/auth-tokens/
export SENTRY_AUTH_TOKEN=your_token_here

# Machine-specific paths (optional)
# Adjust these to match your system or remove if not needed
# export CONSOLE_SESSIONS_LOG_DIR="$HOME/path/to/console-sessions"
